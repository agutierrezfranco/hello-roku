<?xml version="1.0" encoding="utf-8"?>
<component name="itemComponent" extends="group">
    <interface>
        <field id="itemContent" type="node" onChange="onContentChange"/>
        <field id="focusPercent" type="float" onChange="focusPercentChanged"/>
    </interface>
    <script type="text/brightscript" uri="itemComponent.brs" />
    <children>
        <rectangle id="rectangle" color="#FFFFFF" width="800" height="50">
            <label id="label" color="#000000" width="800" height="50" vertAlign="center" horizAlign="left"/>
        </rectangle>
    </children>
</component>